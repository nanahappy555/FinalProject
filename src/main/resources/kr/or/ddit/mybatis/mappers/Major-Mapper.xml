<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="Major-Mapper">

	<select id="selectMajorList" parameterType="String" resultType="major">
		select * from major
		where majorCd=#{majorCd}
	</select>
	
	
	
</mapper>